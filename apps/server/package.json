{
    "name": "@deenruv/server",
    "version": "2.3.1",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "populate": "node -r ts-node/register -r dotenv/config populate-dev-server.ts",
        "run:server": "node -r ts-node/register -r dotenv/config index.ts",
        "run:worker": "node -r ts-node/register -r dotenv/config index-worker.ts",
        "start": "concurrently npm:run*",
        "load-test:1k": "node -r ts-node/register load-testing/run-load-test.ts 1000",
        "load-test:10k": "node -r ts-node/register load-testing/run-load-test.ts 10000",
        "load-test:100k": "node -r ts-node/register load-testing/run-load-test.ts 100000"
    },
    "dependencies": {
        "@nestjs/axios": "^3.0.2",
        "@deenruv/admin-ui-plugin": "workspace:*",
        "@deenruv/asset-server-plugin": "workspace:*",
        "@deenruv/common": "workspace:*",
        "@deenruv/core": "workspace:*",
        "@deenruv/elasticsearch-plugin": "workspace:*",
        "@deenruv/email-plugin": "workspace:*",
        "typeorm": "0.3.20",
        "typescript": "5.3.3"
    },
    "devDependencies": {
        "@deenruv/testing": "workspace:*",
        "@deenruv/ui-devkit": "workspace:*",
        "commander": "^12.0.0",
        "concurrently": "^8.2.2",
        "csv-stringify": "^6.4.6",
        "dayjs": "^1.11.10",
        "progress": "^2.0.3"
    }
}
