{
  "name": "@deenruv/newsletter-plugin",
  "version": "1.0.3",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./plugin-server": "./dist/plugin-server/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --build",
    "watch": "concurrently -k -p \"[{name}]\" -n \"SERVER,UI\" -c \"yellow.bold,cyan.bold\" \"tsc --watch --project src/plugin-server/tsconfig.json\"",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "zeus": "zeus http://localhost:3000/admin-api ./src/plugin-server --es"
  },
  "dependencies": {
    "@deenruv/admin-types": "workspace:^",
    "@deenruv/common": "workspace:^",
    "@deenruv/react-ui-devkit": "workspace:^",
    "@nestjs/common": "~10.3.10",
    "@nestjs/graphql": "~12.2.0",
    "graphql": "~16.9.0",
    "graphql-tag": "^2.12.6",
    "graphql-zeus": "^5.4.2"
  },
  "devDependencies": {
    "@deenruv/core": "workspace:^",
    "@types/express": "^4.17.21",
    "rimraf": "^5.0.10",
    "tslib": "^2.6.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@deenruv/core": "^1.0.0"
  }
}
