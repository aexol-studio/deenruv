{
    "name": "@deenruv/job-queue-plugin",
    "version": "0.0.70",
    "license": "MIT",
    "main": "package/index.js",
    "types": "package/index.d.ts",
    "files": [
        "package/**/*"
    ],
    "private": false,
    "scripts": {
        "watch": "tsc -p ./tsconfig.build.json --watch",
        "build": "rimraf package && tsc -p ./tsconfig.build.json",
