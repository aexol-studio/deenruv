    "@nestjs/graphql": "~12.2.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "graphql": "~16.9.0",
    "graphql-tag": "^2.12.6",
    "graphql-zeus": "^5.4.2",
    "lucide-react": "^0.363.0",
    "react": "^18.2.0",
