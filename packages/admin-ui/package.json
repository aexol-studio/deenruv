{
  "name": "@deenruv/admin-ui",
  "version": "0.1.42",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/set-version.js && ng serve",
    "build:app": "ng build deenruv-admin --configuration production",
    "build": "node scripts/copy-package-json.js && node scripts/set-version.js && node scripts/build-public-api.js && ng build deenruv-admin-lib --configuration production && node scripts/compile-styles.js",
    "watch": "ng build --watch=true",
    "test:ui": "ng test --watch=false --browsers=ChromeHeadlessCI --progress=false",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "extract-translations": "node scripts/extract-translations.js",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postversion": "node ./scripts/set-version.js"
  },
  "homepage": "https://deenruv.com/",
  "publishConfig": {
    "access": "public",
    "directory": "package"
  },
  "//": "These exports are just here so that we can do local testing of ui-devkit compilation by allowing the ui-devkit package to find its way to the compiled package",
  "exports": {
    ".": {
      "types": "./package/index.d.ts",
      "esm2022": "./package/esm2022/deenruv-admin-ui.mjs",
      "esm": "./package/esm2022/deenruv-admin-ui.mjs",
      "default": "./package/fesm2022/deenruv-admin-ui.mjs"
    }
  },
  "dependencies": {
